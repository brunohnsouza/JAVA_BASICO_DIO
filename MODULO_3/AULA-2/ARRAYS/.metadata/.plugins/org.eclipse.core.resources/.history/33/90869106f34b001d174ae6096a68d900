package arraysUnidimensionais;

import java.util.Random;

public class OrdemInversa {
	public static void main(String[] args) {
		
		Random randon = new Random();
		
		int[] numerosInteiros = new int[5];
		
		for (int i = 0; i < numerosInteiros.length; i++) {
			int numero = randon.nextInt(10);
			numerosInteiros[i] = numero;
		}
		
		for (int numero = numerosInteiros.length; numero > numerosInteiros.length; numero--) {
			System.out.print(numerosInteiros[numero]);
		}

	}

}
